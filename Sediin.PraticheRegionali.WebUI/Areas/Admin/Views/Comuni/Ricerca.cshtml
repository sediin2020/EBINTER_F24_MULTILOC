@model  Sediin.PraticheRegionali.WebUI.Areas.Admin.Models.ComuniModel

<div class="row mb-4">
    <div class="col-md-12">
        @Ajax.EncodedAjaxActionLink("Nuovo Comune", "Nuovo", "Comuni", null, new AjaxOptions
                {
                OnBegin = "alertWaid()",
                OnSuccess = "showModalFullScreen_NoHeaderFooter(data); alertClose()",
                OnFailure = "handleError"
                }, new { @class = "btn btn-info" })
    </div>
</div>

@Html.RicercaModulo(Html.Partial("RicercaModulo", Model), "Ricerca Comune")

@Html.UpdateListRicerca(Url.Action("Ricerca", "Comuni"), true)
