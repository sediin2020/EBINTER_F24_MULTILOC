@model  Sediin.PraticheRegionali.WebUI.Areas.Admin.Models.ContatoreAnnualeSearchModel

<div class="row mb-4">
    <div class="col-md-12">
        @Ajax.EncodedAjaxActionLink("Nuovo Contatore", "Nuovo", "ContatoreAnnuale", null, new AjaxOptions
                {
                OnBegin = "alertWaid()",
                OnSuccess = "showModalFullScreen_NoHeaderFooter(data); alertClose()",
                OnFailure = "handleError"
                }, new { @class = "btn btn-info" })
    </div>
</div>

@Html.RicercaModulo(Html.Partial("RicercaModulo", Model), "Ricerca Contatore")

@Html.UpdateListRicerca(Url.Action("Ricerca", "ContatoreAnnuale"), true)
