@model  Sediin.PraticheRegionali.WebUI.Areas.Admin.Models.RequisitiClass

<div class="row mb-4">
    <div class="col-md-12">
        @Ajax.EncodedAjaxActionLink("Nuovo Requisito", "Nuovo", "Requisiti", null, new AjaxOptions
                {
                OnBegin = "alertWaid()",
                OnSuccess = "showModal_NoHeaderFooter(data); alertClose()",
                OnFailure = "handleError"
                }, new { @class = "btn btn-info" })
    </div>
</div>

@Html.RicercaModulo(Html.Partial("RicercaModulo", Model), "Ricerca Requisiti")

@Html.UpdateListRicerca(Url.Action("Ricerca", "Requisiti"), true)
