@model  Sediin.PraticheRegionali.WebUI.Areas.Admin.Models.TipoContrattoSearchModel

<div class="row mb-4">
    <div class="col-md-12">
        @Ajax.EncodedAjaxActionLink("Nuovo Tipo Contratto", "Nuovo", "TipoContratto", null, new AjaxOptions
                {
                OnBegin = "alertWaid()",
                OnSuccess = "showModalFullScreen_NoHeaderFooter(data); alertClose()",
                OnFailure = "handleError"
                }, new { @class = "btn btn-info" })
    </div>
</div>

@Html.RicercaModulo(Html.Partial("RicercaModulo", Model), "Ricerca Tipo Contratto")

@Html.UpdateListRicerca(Url.Action("Ricerca", "TipoContratto"), true)
