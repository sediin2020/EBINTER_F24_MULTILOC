@model  Sediin.PraticheRegionali.WebUI.Areas.Admin.Models.DichiarazioniDPRClass

<div class="row mb-4">
    <div class="col-md-12">
        @Ajax.EncodedAjaxActionLink("Nuovo Dichiarazione DPR", "Nuovo", "DichiarazioniDPR", null, new AjaxOptions
                {
                OnBegin = "alertWaid()",
                OnSuccess = "showModal_NoHeaderFooter(data); alertClose()",
                OnFailure = "handleError"
                }, new { @class = "btn btn-info" })
    </div>
</div>

@Html.RicercaModulo(Html.Partial("RicercaModulo", Model), "Ricerca Dichiarazioni DPR")

@Html.UpdateListRicerca(Url.Action("Ricerca", "DichiarazioniDPR"), true)
