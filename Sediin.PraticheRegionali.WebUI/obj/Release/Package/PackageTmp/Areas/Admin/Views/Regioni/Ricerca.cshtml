@model  Sediin.PraticheRegionali.WebUI.Areas.Admin.Models.RegioniModel

<div class="row mb-4">
    <div class="col-md-12">
        @Ajax.EncodedAjaxActionLink("Nuova Regione", "Nuovo", "Regioni", null, new AjaxOptions
                {
                OnBegin = "alertWaid()",
                OnSuccess = "showModalFullScreen_NoHeaderFooter(data); alertClose()",
                OnFailure = "handleError"
                }, new { @class = "btn btn-info" })
    </div>
</div>

@Html.RicercaModulo(Html.Partial("RicercaModulo", Model), "Ricerca Regioni")

@Html.UpdateListRicerca(Url.Action("Ricerca", "Regioni"), true)
